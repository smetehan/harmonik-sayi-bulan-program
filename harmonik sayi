import java.util.Scanner;

public class project {
    public static void main(String[] args) {
        Scanner inp = new Scanner(System.in);

      int n;
      double total=0;
        System.out.print("N sayisini giriniz :");
        n= inp.nextInt();
        for(double i=1; i<=n; i++){

            total= total +(1/i);

        } System.out.print(total);
    }
}
